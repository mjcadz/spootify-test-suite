{
  "name": "spootify-test-suite",
  "version": "1.0.0",
  "description": "Testing Code Challenge - Test Automation Engineer",
  "scripts": {
    "e2e:web": "cypress run --browser chrome --env viewport=web",
    "e2e:mobile": "cypress run --browser chrome --env viewport=mobile",
    "e2e:gui:web": "cypress open --env viewport=web",
    "e2e:gui:mobile": "cypress open --env viewport=mobile",
    "e2e:record:web": "cypress run --record --browser chrome --env viewport=web",
    "e2e:record:mobile": "cypress run --record --browser chrome --env viewport=mobile"
  },
  "author": "Michael Cadzow",
  "license": "ISC",
  "dependencies": {
    "cypress": "^5.4.0"
  }
}
